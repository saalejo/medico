<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="logistico.db.dto.modelo.Compania" table="LO_COMPANIA">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="nit" type="java.lang.String">
            <column name="NIT" />
        </property>
        <property name="direccion" type="java.lang.String">
            <column name="DIR" />
        </property>
        <property name="telefono" type="java.lang.String">
            <column name="TEL" />
        </property>
        <property name="codigoHabilitacionIps" type="java.lang.String">
            <column name="COD_IPS" />
        </property>
        <property name="sloganIps" type="java.lang.String">
            <column name="SLG" />
        </property>
        <property name="usuarioConectado" type="int">
            <column name="USU_CON" />
        </property>
        <many-to-one name="departamento" class="logistico.db.dto.modelo.Departamento" fetch="join" insert="false" update="false">
            <column name="LO_DEPARTAMENTO_ID" />
        </many-to-one>
        <property name="departamentoId" type="int">
            <column name="LO_DEPARTAMENTO_ID" />
        </property>
        <many-to-one name="municipio" class="logistico.db.dto.modelo.Municipio" fetch="join" insert="false" update="false">
            <column name="LO_MUNICIPIO_ID" />
        </many-to-one>
        <property name="municipioId" type="int">
            <column name="LO_MUNICIPIO_ID" />
        </property>
        <many-to-one name="barrio" class="logistico.db.dto.modelo.Barrio" fetch="join" insert="false" update="false">
            <column name="LO_BARRIO_ID" />
        </many-to-one>
        <property name="barrioId" type="int">
            <column name="LO_BARRIO_ID" />
        </property>
    </class>
</hibernate-mapping>