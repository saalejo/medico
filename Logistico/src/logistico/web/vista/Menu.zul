<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>
<borderlayout apply="${menuLogisticoCtl}" >
	<north border="0">
		<borderlayout height="30px">
			<center border="0">
				<hlayout spacing="5px" style="background: #C9C9C9;">
					<menubar >	
						<menu label="${labels.logistico.menu.Nuevo}">
		                <menupopup>	
	                    	<menuitem id="btnNuevaCompania" label="${labels.logistico.menu.nuevo.Compania}" image="/res/plus.png" />	
	                        <menuitem id="btnNuevaSede" label="${labels.logistico.menu.nuevo.Sede}" image="/res/plus.png" />
	                        <menuitem id="btnNuevoBarrio" label="${labels.logistico.menu.nuevo.Barrio}" image="/res/plus.png" />	
	                        <menuseparator></menuseparator>		                        
	                        <menuitem id="btnNuevoDepartamento" label="${labels.logistico.menu.nuevo.Departamento}" image="/res/plus.png" />
	                        <menuitem id="btnNuevaMunicipio" label="${labels.logistico.menu.nuevo.Municipio}" image="/res/plus.png" />
	                        <menuitem id="btnNuevaProfesional" label="${labels.logistico.menu.nuevo.Profesional}" image="/res/plus.png" />
	                        <menuseparator></menuseparator>
	                        <menuitem id="btnNuevoProfesionalEspecialidad" label="${labels.logistico.menu.nuevo.ProfesionalEspecialidad}" image="/res/plus.png" />		                        
	                        <menuitem id="btnNuevaProtocolo" label="${labels.logistico.menu.nuevo.Protocolo}" image="/res/plus.png" />		                        
		                </menupopup>
		                </menu>
		               	<menuitem id="btnListaCompania" label="${labels.logistico.menu.ListaCompania}"  image="/res/fn.png" />
		                <menuitem id="btnListaSede" label="${labels.logistico.menu.ListaSede}"  image="/res/fn.png" />
		                <menuitem id="btnListaBarrio" label="${labels.logistico.menu.ListaBarrio}"  image="/res/fn.png" />
		                <menuitem id="btnListaDepartamento" label="${labels.logistico.menu.ListaDepartamento}"  image="/res/fn.png" />
		                <menuitem id="btnListaMunicipio" label="${labels.logistico.menu.ListaMunicipio}"  image="/res/fn.png" />
		                <menuitem id="btnListaProfesional" label="${labels.logistico.menu.ListaProfesional}"  image="/res/fn.png" />
		                <menuitem id="btnListaProfesionalEspecialidad" label="${labels.logistico.menu.ListaProfesionalEspecialidad}"  image="/res/fn.png" />
		                <menuitem id="btnListaProtocolo" label="${labels.logistico.menu.ListaProtocolo}"  image="/res/fn.png" />
					</menubar>
				</hlayout>
			</center>			
		</borderlayout>
	</north>
	<center border="0">
    	<window id="contenido" style="width:100%; height:100%;" />
	</center>
</borderlayout>