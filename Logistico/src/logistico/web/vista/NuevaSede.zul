				<row>
					<cell>
						<label value="${labels.logistico.sede.Telefono}"/>
					</cell>
					<cell>
						<textbox id="telefono" readonly="false" constraint="no empty: no puede ser vacio" width="90%" />
					</cell>	
					<cell>
						<label value="${labels.logistico.sede.CodigoHabilitacionIps}"/>
					</cell>
					<cell>
						<textbox id="codigoHabilitacionIps" readonly="false" constraint="no empty: no puede ser vacio" width="90%" />
