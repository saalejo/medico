				</row>
				<row>
					<cell>
						<label value="${labels.logistico.sede.Compania}"/>
					</cell>
					<cell>
						<textbox id="compania" readonly="false" constraint="no empty: no puede ser vacio" width="90%" />
					</cell>	
					<cell>
						<label value="${labels.logistico.sede.Departamento}"/>
					</cell>
					<cell>
						<textbox id="departamento" readonly="false" constraint="no empty: no puede ser vacio" width="90%" />
					</cell>	
				</row>
				<row>
					<cell>
						<label value="${labels.logistico.sede.Compania}"/>
					</cell>
					<cell>
						<textbox id="compania" readonly="false" constraint="no empty: no puede ser vacio" width="90%" />
					</cell>	
					<cell>
										
					</cell>
					<cell>
						<button id="cancelar" label="${labels.general.Cancelar}"></button>
						<button id="guardar" label="${labels.general.Guardar}"></button>
					</cell>
				</row>
			</rows>
		</grid>
	</window>
</zk>