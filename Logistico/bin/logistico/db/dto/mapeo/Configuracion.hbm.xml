<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="logistico.db.dto.modelo.Configuracion" table="LO_CONFIGURACION">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="mensaje" type="java.lang.String">
            <column name="MEN" />
        </property>
        <property name="usuarioConectado" type="int">
            <column name="USU_CON" />
        </property>
        <many-to-one name="moduloSistema" class="logistico.db.dto.modelo.ModuloSistema" fetch="join" insert="false" update="false">
            <column name="LO_MODULO_ID" />
        </many-to-one>
        <property name="moduloSistemaId" type="int">
            <column name="LO_MODULO_ID" />
        </property>
        <many-to-one name="especialidad" class="historia.clinica.db.dto.modelo.Especialidad" fetch="join" insert="false" update="false">
            <column name="HC_ESPECIALIDAD_ID" />
        </many-to-one>
        <property name="especialidadId" type="int">
            <column name="HC_ESPECIALIDAD_ID" />
        </property>
    </class>
</hibernate-mapping>
