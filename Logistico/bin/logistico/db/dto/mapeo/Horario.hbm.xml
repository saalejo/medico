<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="logistico.db.dto.modelo.Configuracion" table="LO_CONFIGURACION">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="horaInicial" type="java.lang.Date">
            <column name="HOR_INI" />
        </property>
         <property name="horaFinal" type="java.lang.Date">
            <column name="HOR_FIN" />
        </property>
        <property name="dia" type="java.lang.String">
            <column name="DIA" />
        </property>
        <property name="activo" type="java.lang.String">
            <column name="ACT" />
        </property>
        <property name="usuarioConectado" type="int">
            <column name="USU_CON" />
        </property>
    </class>
</hibernate-mapping>
