<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="historia.clinica.db.dto.modelo.Remision" table="remision">
        <id name="id" type="int">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="motivo" type="java.lang.String">
            <column name="motivo" />
        </property>
        <property name="resultadoExamenes" type="java.lang.String">
            <column name="resultado_examenes" />
        </property>
        <property name="atencionId" type="int">
            <column name="atencion_id" />
        </property>
        <property name="usuarioConectado" type="java.lang.String">
            <column name="usuario_conectado" />
        </property>
    </class>
</hibernate-mapping>
