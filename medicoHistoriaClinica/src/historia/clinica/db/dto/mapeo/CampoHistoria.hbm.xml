<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="historia.clinica.db.dto.modelo.CampoHistoria" table="CAMPOS_HISTORIA">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="clave" type="java.lang.String">
            <column name="CLAVE" />
        </property>
        <property name="valor" type="java.lang.String">
            <column name="VALOR" />
        </property>
        <property name="observaciones" type="java.lang.String">
            <column name="OBSERVACIONES" />
        </property>
        <property name="padre" type="java.lang.String">
            <column name="padre" />
        </property>
        <property name="atencionId" type="int">
            <column name="atencion_id" />
        </property>
    </class>
</hibernate-mapping>
