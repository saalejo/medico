<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="admisiones.db.dto.modelo.Prestador" table="prestador">
        <id name="id" type="int">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="nombre" type="java.lang.String">
            <column name="nombre" />
        </property>
        <property name="identificacion" type="java.lang.String">
            <column name="identificacion" />
        </property>
        <property name="razonSocial" type="java.lang.String">
            <column name="razon_social" />
        </property>
        <property name="tipoIdentificacion" type="java.lang.String">
            <column name="tipo_identificacion" />
        </property>
        <property name="municipioId" type="int">
            <column name="municipio_id" />
        </property>
        <property name="usuarioConectado" type="java.lang.String">
            <column name="usuario_conectado" />
        </property>
        <many-to-one name="municipio" class="admisiones.db.dto.modelo.Municipio" fetch="join" insert="false" update="false">
            <column name="municipio_id" />
        </many-to-one>
    </class>
</hibernate-mapping>
