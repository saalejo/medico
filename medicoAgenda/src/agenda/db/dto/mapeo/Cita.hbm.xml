<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="agenda.db.dto.modelo.Cita" table="cita">
        <id name="id" type="int">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="pacienteId" type="int">
            <column name="paciente_id" />
        </property>        
        <property name="usuarioLogueado" type="int">
       		<column name="usuario_logueado" />
        </property>
        <property name="hora" type="java.util.Date">
            <column name="hora" />
        </property>
        <property name="turnoId" type="int">
            <column name="turno_id" />
        </property>
    </class>
</hibernate-mapping>
